{
    "presets": [
        "react", [
            "env", {
                "modules": false
            }
        ],
        "stage-2"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        "transform-decorators-legacy",
        "transform-regenerator",
        "react-hot-loader/babel",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
    ]
}