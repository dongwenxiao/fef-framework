{
    "presets": [
        "react", [
            "env", {
                "modules": false
            }
        ],
        "stage-0"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        "transform-regenerator"
    ]
}